          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".second-element-box, .image-wrap, .tabContent": {
              "width": "100%"
            },
            ".second-element, .second-element-box, .image-wrap": {
              "position": "relative"
            },
            ".tabContent .second-element when (@showPadding)": {
              "@media (min-width: 992px)": {
                "& when not (@reverseContent)": {
                  "padding-left": "40px"
                },
                "& when (@reverseContent)": {
                  "padding-right": "40px"
                }
              },
              "@media (min-width: 1200px)": {
                "& when (@fullWidth)": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".tabContent when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "padding": "0"
              }
            },
            "amp-img,img": {
              "object-fit": "cover",
              "@media (min-width: 768px)": {
                "height": "~\"@{heightImg}rem\""
              }
            },
            ".milestone:before when (@showBorderTop) and (@showMilestone)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            ".list1 .counters-wrap:before, .list1 .marker-wrap:before, .list2 .line-wrap:before, .list2 .marker-wrap:before": {
              "display": "inline-block"
            },
            ".list1 when (@showCardList1)": {
              "counter-reset": "decimal",
              ".counters-wrap:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg1Counters}rem\"",
                "line-height": "1.7",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor",
                  "line-height": "1.4"
                },
                "& when ((@widthImg1Counters * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px"
                  }
                }
              },
              ".marker-wrap:before": {
                "content": "\"\"",
                "border": "3px solid @primaryColor",
                "width": "~\"calc(@{widthImg1Markers}rem - 6px)\"",
                "height": "~\"calc(@{widthImg1Markers}rem - 6px)\"",
                "box-sizing": "initial",
                "& when ((@widthImg1Markers * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "height": "30px",
                    "width": "30px"
                  }
                }
              },
              ".icon-wrap .amp-iconfont": {
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg1Icons}rem\"",
                "width": "~\"@{widthImg1Icons}rem\"",
                "& when ((@widthImg1Icons * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px",
                    "width": "30px"
                  }
                }
              },
              ".marker-wrap, .icon-wrap, .counters-wrap": {
                "margin-right": "15px"
              },
              ".items-list:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".items-list": {
              "flex-direction": "row"
            },
            "@media (max-width: 767px)": {
              ".second-element when (@transparentBg)": {
                "padding-top": "50px"
              },
              ".milestone when (@showMilestone)": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:first-child)": {
                  "margin-top": "20px"
                }
              }
            },
            ".list2 .line-wrap:before, .list1 .marker-wrap:before": {
              "margin-top": "9px"
            },
            ".list2 when (@showCardList2)": {
              ".line-wrap:before": {
                "content": "\"\"",
                "background-color": "@primaryColor",
                "margin-right": "5px",
                "width": "~\"@{widthImg2}rem\"",
                "height": "3px",
                "& when ((@widthImg2 * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "width": "30px"
                  }
                }
              },
              ".marker-wrap:before": {
                "content": "\"âœ“\"",
                "font-size": "~\"@{widthImg2}rem\"",
                "margin": "6px 13px 0 0 ",
                "& when ((@widthImg2 * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px"
                  }
                }
              }
            },
            "@media (min-width: 768px)": {
              ".ampTabContainer": {
                "display": "flex",
                "flex-wrap": "wrap",
                "padding": "0"
              },
              ".milestone > .mbr-col-md-6 when (@showMilestone)": {
                "&:nth-child(2) when (@columnWidth = 5)": {
