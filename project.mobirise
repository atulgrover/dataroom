            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@paddingTop < 3), (@paddingBottom < 3)": {
              "overflow": "visible"
            },
            ".form__wrap": {
              "background-color": "@formColor",
              "box-shadow": "0 0 35px 0 rgba(0, 0, 0, 0.15)",
              "padding": "60px 35px"
            },
            ".field": {
              "position": "relative",
              "padding": "15px 25px",
              "margin-bottom": "0 !important"
            },
