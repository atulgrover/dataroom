            },
            ".tab-title": {
              "position": "relative",
              "min-height": "35px",
              "max-height": "110px",
              "padding-left": "45px",
              "background": "none",
              "border": "none",
              "transition": "0.4s"
            },
            ".tab-title::before": {
              "content": "'+'",
              "position": "absolute",
              "transition": "all .6s",
              "width": "30px",
              "height": "30px",
              "color": "#fff",
              "background-color": "@tabsIconColor",
              "font-weight": "700",
              "font-size": "25px",
              "display": "inline-flex",
              "justify-content": "center",
              "align-items": "center",
              "left": "0",
              "padding-bottom": "2px",
              "padding-left": "1px"
            },
